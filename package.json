{
  "name": "@josselinbuils/utils",
  "version": "0.1.2",
  "author": "Josselin BUILS",
  "license": "MIT",
  "description": "Light JS/TS utils library.",
  "scripts": {
    "doc": "api-documenter",
    "build": "rimraf dist && rollup -c && api-extractor run --local --verbose && rimraf dist/src",
    "lint": "tslint -p tsconfig.json",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && pretty-quick --staged --pattern \"**/*.ts\""
    }
  },
  "devDependencies": {
    "@josselinbuils/api-documenter": "^0.4.0",
    "@josselinbuils/tslint": "^2.0.0",
    "@microsoft/api-extractor": "^7.1.5",
    "@types/jest": "^24.0.13",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.11.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-jest": "^24.0.2",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/josselinbuils/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josselinbuils/utils.git"
  },
  "bugs": {
    "url": "https://github.com/josselinbuils/utils/issues"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {}
}
